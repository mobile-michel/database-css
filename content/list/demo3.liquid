---
title: Third Demo
description: This is the third demo with 8 ARIA landmarks
pagination:
  data: test
  size: 1
  alias: html
permalink: list/demo3/{{ html.name | slug }}/index.html
aside: true
---
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{ html.name }}</title>
  <link rel="stylesheet" href="/assets/css/{{ html.name }}.css">
  <link rel="stylesheet" href="/assets/css/colors.css">
</head>

<body>
  <header>
    {% include 'nav-primary' %}
    <h1>{{ pagination.items[0].name | capitalize }} Framework</h1>
  </header>
  <main>
    {% if aside %}
    <aside>
      {% include 'secondary-links' %}
    </aside>
    {% endif %}
    {% include 'pagination' %}
    <section aria-labelledby="{{ title | slugify }}">
      <h2 id="{{ title | slugify }}">{{ title }}</h2>
      <p>This is a section HTML5 semantic element.</p>
    </section>
    <article>
      <h3>Article html5 semantic element</h3>
      {% include 'elements' %}
    </article>
  </main>
  <footer>
    {% include 'nav-footer' %}
    {% include 'copyright' %}
  </footer>

</body>

</html>