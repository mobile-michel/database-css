---
title: Demo
pagination:
  data: stylesheet
  size: 1
  alias: html
permalink: list/demo2/{{ html.name | slug }}/index.html
---
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{ html.name }}</title>
  <link rel="stylesheet" href="/assets/css/{{ html.name }}.css">
  <link rel="stylesheet" href="/assets/css/colors.css">
</head>

<body>
  <header>
    {% include 'nav-primary' %}
    <h1>{{ html.name }}</h1>
  </header>
  <main>
    <section aria-labelledby="{{ title | slugify }}">
      {% include 'pagination' %}
      <h2 id="{{ title | slugify }}">{{ title }}</h2>
      <p>This is inside a <code>section</code> HTML element.</p>
      <p>{{ html.description }}</p>
      <p>This framework has {{ html.stars }} stars on Github.</p>
      <p>Version {{ html.version }} was published on {{ html.published | date: "%d %b %Y" }}.</p>
    </section>
    <aside>
      <header>
        <h3>This is an aside</h3>
      </header>
      <nav aria-label="Secondary">
        <ul>
          <li><a href="{{ html.url | url }}">Website</a></li>
          <li><a href="{{ html.github | url }}">Github</a></li>
        </ul>
      </nav>
    </aside>
    <article>
      <h3>HTML elements</h3>
      <p>This is inside an <code>article</code> semantic HTML element</p>
      <ul>
        <li><strong>landmarks elements</strong>: {{ html.landmarks }}%</li>
        <li><strong>elements name</strong>: {{ html.landmarks-name }}</li>
        <li><strong>HTML elements</strong>: {{ html.html-elements }}%</li>
        <li><strong>stylesheet</strong>: <a href="/assets/css/{{ html.name }}.css" target="_blank">/assets/css/{{ html.name }}.css</a></li>
        <li><strong>primary navigation</strong>: {{ html.nav }}</li>
        <li><strong>centered content</strong>: {{ html.center }}</li>
        <li><strong>display: grid</strong>: {{ html.grid }}</li>
        <li><strong>display: flex</strong>: {{ html.flex }}</li>
        <li><strong>float</strong>: {{ html.float }}</li>
        <li><strong>remarks</strong>: {{ html.remarks }}</li>
      </ul>
    </article>
  </main>
  <footer>
    {% include 'nav-footer' %}
    <small>&copy; {{ "now" | date: "%Y" }} footer</small>
  </footer>
</body>

</html>